[
    {
        "id": 1,
        "title": "Bersakit-Sakit Dahulu",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Berakit-rakit ke hulu,",
            "Berenang-renang ke tepian.",
            "Bersakit-sakit dahulu,",
            "Bersenang-senang kemudian."
        ]
    },
    {
        "id": 2,
        "title": "Kalau Salah, Tolong Tunjukkan",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Buah cempedak di luar pagar,",
            "Ambil galah tolong jolokkan.",
            "Hamba budak baru belajar,",
            "Kalau salah, tolong tunjukkan."
        ]
    },
    {
        "id": 3,
        "title": "Yang Indah Itu Bahasa",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Yang kurik itu kundi,",
            "Yang merah itu saga.",
            "Yang baik itu budi,",
            "Yang indah itu bahasa."
        ]
    },
    {
        "id": 4,
        "title": "Hutang Budi Dibawa Mati",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Pisang emas dibawa belayar,",
            "Masak sebiji di atas peti.",
            "Hutang emas boleh dibayar,",
            "Hutang budi dibawa mati."
        ]
    },
    {
        "id": 5,
        "title": "Budi Yang Baik Dikenang Jua",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Pulau Pandan jauh ke tengah,",
            "Gunung Daik bercabang tiga.",
            "Hancur badan dikandung tanah,",
            "Budi yang baik dikenang jua."
        ]
    },
    {
        "id": 6,
        "title": "Suruh Mengaji Saya Tak Tahu",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Sorong papan, tarik papan,",
            "Buah keranji di dalam perahu.",
            "Suruh makan, saya makan,",
            "Suruh mengaji, saya tak tahu."
        ]
    },
    {
        "id": 7,
        "title": "Dunia Itu Main-Mainan",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Peria itu tanam-tanaman,",
            "Di darat juga tumbuhnya.",
            "Dunia itu main-mainan,",
            "Akhirat juga sungguhnya."
        ]
    },
    {
        "id": 8,
        "title": "Pilih-Pilih Sahabat Berbudi",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Pilih-pilih tempat menjadi,",
            "Pertama teluk, kedua pantai.",
            "Pilih-pilih sahabat berbudi,",
            "Pertama elok, kedua pandai."
        ]
    },
    {
        "id": 9,
        "title": "Apakah Tanda Orang Berbudi?",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Apakah tanda kerang berisi?",
            "Bila direbus, kulitnya merekah.",
            "Apakah tannda orang berbudi?",
            "Bila disebut, dirinya merendah."
        ]
    },
    {
        "id": 10,
        "title": "Budi Yang Baik Dikenang Juga",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Pulau Pandan jauh ke tengah,",
            "Gunung Daik bercabang tiga.",
            "Hancur badan dikandung tanah,",
            "Budi yang baik dikenang juga."
        ]
    },
    {
        "id": 11,
        "title": "Ilmu Itu Pelindung Diri",
        "section": "Nasihat",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Ilmu itu pelindung diri,",
            "Ibarat kebun berpagar duri.",
            "Seperti sinar intan baiduri,",
            "Itulah tanda orang jauhari."
        ]
    }
]