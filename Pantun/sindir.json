[
    {
        "id": 1,
        "title": "Kajang Pak Malau",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Kajang Pak Malau, kajang berlipat,",
            "Kajang hamba, mengkuang layu.",
            "Dagang Pak Malau, dagang bertempat,",
            "Dagang hamba, musafir lalu."
        ]
    },
    {
        "id": 2,
        "title": "Kalau Jadi Katakan Jadi",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Kalau padi katakan padi,",
            "Tidak hamba tertampi-tampi.",
            "Kalau jadi katakan jadi,",
            "Tidak hamba ternanti-nanti."
        ]
    },
    {
        "id": 3,
        "title": "Jiwa Dan Raga, Hamba Serahkan",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Jikalau runtuh Kota Melaka,",
            "Papan di Jawa hamba dirikan.",
            "Jikalau sungguh bagai dikata,",
            "Jiwa dan raga, hamba serahkan."
        ]
    },
    {
        "id": 4,
        "title": "Esok Jangan Rindu-Rinduan",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Tuai padi antara masak,",
            "Esok jangan layu-layuan.",
            "Intai kami antara nampak,",
            "Esok jangan rindu-rinduan."
        ]
    },
    {
        "id": 5,
        "title": "Jangan Disimpan Di Dalam Hati",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Jikalau ada jarum yang patah,",
            "Jangan disimpan di dalam peti.",
            "Jikalau ada silap dan salah,",
            "Jangan disimpan di dalam hati."
        ]
    },
    {
        "id": 6,
        "title": "Hari Esok Kita Bersurai",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Hari ini menanam jagung,",
            "Hari esok menanam serai.",
            "Hari ini kita sekampung,",
            "Hari esok kita bersurai."
        ]
    },
    {
        "id": 7,
        "title": "Dalam Pantun Ada Bicara",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Minta daun diberi kebun,",
            "Dalam kebun ada bidara.",
            "Minta santun diberi pantun,",
            "Dalam pantun ada bicara."
        ]
    },
    {
        "id": 8,
        "title": "Habis Budi, Kami Dibuang",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Apalah diharapkan jerami?",
            "Habis padi, jerami dibuang.",
            "Apalah diharapkan kami?",
            "Habis budi, kami dibuang."
        ]
    },
    {
        "id": 9,
        "title": "Kalau Tak Sungguh, Tak Mengata",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Kalau tak tumbuh, tak melata,",
            "Kalau tak sungguh, tak mengata."
        ]
    }, 
    {
        "id": 10,
        "title": "Dalam Pantun Menanam Makna",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Orang berkebun, saya berkebun,",
            "Dalam kebun, menanam bunga.",
            "Orang berpantun, saya berpantun,",
            "Dalam pantun, menanam makna."
        ]
    }, 
    {
        "id": 11,
        "title": "Di Mulut Naga Lagikan Kurebut",
        "section": "Sindir",
        "source": "Pantun Rumpun Melayu",
        "content": [
            "Daun cempaka dipukul ribut,",
            "Dibawa orang tanah seberang.",
            "Di mulut naga lagikan kurebut,",
            "Inikan pula di tangan orang."
        ]
    }
]
